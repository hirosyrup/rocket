{
  "manifest_version": 2,
  "name": "rocket",
  "version": "1.0.0",

  "description": "設定したURLを新しいウィンドウで開く",

  "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
  },
  
  "browser_action": {
    "default_title": "",
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
	  "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.min.js", "content.js"],
	  "run_at": "document_end"
    }
  ],
  
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}